@Article{Hanigan2012e,
  abstract = {There is concern in Australia that droughts substantially increase the incidence of suicide in rural populations, particularly among male farmers and their families. We investigated this possibility for the state of New South Wales (NSW), Australia between 1970 and 2007, analyzing data on suicides with a previously established climatic drought index. Using a generalized additive model that controlled for season, region, and long-term suicide trends, we found an increased relative risk of suicide of 15{\%} (95{\%} confidence interval, 8{\%}-22{\%}) for rural males aged 30-49 y when the drought index rose from the first quartile to the third quartile. In contrast, the risk of suicide for rural females aged >30 y declined with increased values of the drought index. We also observed an increased risk of suicide in spring and early summer. In addition there was a smaller association during unusually warm months at any time of year. The spring suicide increase is well documented in nontropical locations, although its cause is unknown. The possible increased risk of suicide during drought in rural Australia warrants public health focus and concern, as does the annual, predictable increase seen each spring and early summer. Suicide is a complex phenomenon with many interacting social, environmental, and biological causal factors. The relationship between drought and suicide is best understood using a holistic framework. Climate change projections suggest increased frequency and severity of droughts in NSW, accompanied and exacerbated by rising temperatures. Elucidating the relationships between drought and mental health will help facilitate adaptation to climate change.},
  author = {I. C. Hanigan and C. D. Butler and P. N. Kokic and M. F. Hutchinson},
  doi = {10.1073/pnas.1112965109},
  file = {:home/ivan{\_}hanigan/references/Hanigan et al. - 2012 - Suicide and drought in New South Wales, Australia, 1970-2007.pdf:pdf},
  isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
  issn = {0027-8424},
  journal = {Proceedings of the National Academy of Sciences},
  keywords = {Adaptation,Adult,Agriculture,Agriculture: statistics {\&} numerical data,Climate Change,Climate Change: statistics {\&} numerical data,Depressive Disorder,Depressive Disorder: epidemiology,Depressive Disorder: psychology,Droughts,Droughts: statistics {\&} numerical data,Female,Humans,Incidence,Male,Middle Aged,New South Wales,New South Wales: epidemiology,Psychological,Rain,Risk Factors,Rural Population,Rural Population: statistics {\&} numerical data,Suicide,Suicide: psychology,Suicide: statistics {\&} numerical data,Suicide: trends},
  month = {aug},
  number = {35},
  pages = {13950--13955},
  pmid = {22891347},
  title = {{Suicide and drought in New South Wales, Australia, 1970-2007}},
  url = {http://www.pnas.org/cgi/content/abstract/1112965109v1 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3435226{\&}tool=pmcentrez{\&}rendertype=abstract},
  volume = {109},
  year = {2012},
}

@Article{Vins2015,
  abstract = {Little is understood about the long term, indirect health consequences of drought (a period of abnormally dry weather). In particular, the implications of drought for mental health via pathways such as loss of livelihood, diminished social support, and rupture of place bonds have not been extensively studied, leaving a knowledge gap for practitioners and researchers alike. A systematic review of literature was performed to examine the mental health effects of drought. The systematic review results were synthesized to create a causal process diagram that illustrates the pathways linking drought effects to mental health outcomes. Eighty-two articles using a variety of methods in different contexts were gathered from the systematic review. The pathways in the causal process diagram with greatest support in the literature are those focusing on the economic and migratory effects of drought. The diagram highlights the complexity of the relationships between drought and mental health, including the multiple ways that factors can interact and lead to various outcomes. The systematic review and resulting causal process diagram can be used in both practice and theory, including prevention planning, public health programming, vulnerability and risk assessment, and research question guidance. The use of a causal process diagram provides a much needed avenue for integrating the findings of diverse research to further the understanding of the mental health implications of drought.},
  author = {Holly Vins and Jesse Bell and Shubhayu Saha and Jeremy Hess},
  doi = {10.3390/ijerph121013251},
  file = {:home/ivan{\_}hanigan/references/Vins et al. - 2015 - The mental health outcomes of drought A systematic review and causal process diagram.pdf:pdf},
  issn = {1660-4601},
  journal = {International Journal of Environmental Research and Public Health},
  keywords = {causal process diagram,climate change,drought,human health,mental health},
  number = {10},
  pages = {13251--13275},
  title = {{The mental health outcomes of drought: A systematic review and causal process diagram}},
  url = {http://www.mdpi.com/1660-4601/12/10/13251/},
  volume = {12},
  year = {2015},
}


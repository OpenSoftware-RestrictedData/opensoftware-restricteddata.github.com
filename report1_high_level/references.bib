  isbn = {1872258131},
  issn = {1660-4601},
  journal = {International Journal of Environmental Research and Public Health},
  keywords = {big data,climate change,data linkage,data platforms,ecological public health,environmental change,environmental health,evidence base,surveillance systems,vulnerable populations},
  number = {2},
  pages = {1725--1746},
  title = {{Data mashups: Potential contribution to decision support on climate change and health}},
